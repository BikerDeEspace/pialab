pipeline {
    agent  any
    
    stages {
        stage ('Build') {
            steps {
                build job: "Pialab/Pialab-Front-Build",  parameters: [
                   string(value: "${env.BRANCH_NAME}",  name: 'BRANCH_NAME'),
                   string(value: "${env.CHANGE_ID}",  name: 'PR_ID'),
                ], wait: true  
            }
        }
    }
}
